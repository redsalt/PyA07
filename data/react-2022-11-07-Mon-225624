<script>
				var recaptchaLoaded=false;

				$(function() {
					setTimeout(function() {
						console.log('loading recaptcha');
						$('html').append('<script src="https://www.google.com/recaptcha/api.js?render=6Ld_BecUAAAAAAxzQwBa6NY3-m5RDmjVY__ScSww&onload=recaptchaCallback" async defer>'+decodeURI('%3C/script%3E'));
					},3000);

					$('.catch-emails .email').bind('keyup',function(e) {
						if(!recaptchaLoaded && e.which==13) {
							setTimeout(function() {
								$('.action-subscribe').click();
							},1000);
						}
					});
					$('.action-subscribe').bind('click',function(e) {
						if(!recaptchaLoaded) {
							setTimeout(function() {
								$('.action-subscribe').click();
							},1000);
						}
					});
				});
				
				var recaptchaCallback = function() {
					recaptchaLoaded=true;
					$(function() {
						$('.catch-emails .email').bind('keyup',function(e) {
							if(e.which===13) {
								$('.catch-emails').hide();
								grecaptcha.ready(function() {
								grecaptcha.execute('6Ld_BecUAAAAAAxzQwBa6NY3-m5RDmjVY__ScSww', {action: 'homepage'}).then(function(recaptcha_token) {
										submitSubscribeForm(recaptcha_token);
									});
								});
							}
						});

						$('.action-subscribe').bind('click',function() {
							$('.catch-emails').hide();
							grecaptcha.ready(function() {
							grecaptcha.execute('6Ld_BecUAAAAAAxzQwBa6NY3-m5RDmjVY__ScSww', {action: 'homepage'}).then(function(recaptcha_token) {
								submitSubscribeForm(recaptcha_token);
								});
							});
						});
					});
				};

				function submitSubscribeForm(recaptcha_token) {

					var spamCheckAnswer=prompt("To avoid spam sign ups, please answer: What is 1+1?");
					if(
						spamCheckAnswer!=2 &&
						spamCheckAnswer!='2' &&
						spamCheckAnswer!='two'
					) {
						return;
					}
					
					console.log('submitSubscribeForm');
					$('html').addClass('loading');
					$.ajax({
						url: '/emails/subscribe',
						type:'GET',
						dataType:'json',
						data:{
							format:'json',
							spam_check_answer:spamCheckAnswer,
							TIME:$('.catch-emails .time').val(),
							FNAME:$('.catch-emails .name').val(),
							EMAIL:$('.catch-emails .email').val(),
							SOURCE:$('.catch-emails .source').val(),
							TAG:$('.catch-emails .tag').val(),
							recaptcha:recaptcha_token
						},
					}).done(function(reply) {
						$('html').removeClass('loading');
						if(reply.success) {
							if(!empty(pageTagTitle)) {
								setCookie('subscribed_to_newsletter_2020fix_'+pageTagTitle, 'true', 365,'/','.remoteok.com');
							}
							else {
								setCookie('subscribed_to_newsletter', 'true', 365,'/','.remoteok.com');
							}
							$('html').removeClass('catch-emails-enabled');
							alert(reply.message);
							// redir after subscribing to newsletter cuz user clicked [ create profile ]
							// "We'll now redirect you so you can sign up to get a remote worker profile and get even more targeted remote job matches."
							window.location.href='/sign-up?email='+encodeURIComponent($('.form .email').val())+'#worker';
						}
						else {
							$('.catch-emails').show();
							if(!empty(reply.message)) {
								alert(reply.message);
							}
							else {
								alert("Something went wrong!\n\nTweet @remoteok if this keeps happening!");
							}
						}
					});
				}
			</script>
</div>
</div>
<script>

	function windowSize() {
		var e = window, a = 'inner';
		if (!('innerWidth' in window )) {
				a = 'client';
				e = document.documentElement || document.body;
		}
		return { width : e[ a+'Width' ] , height : e[ a+'Height' ] };
	}
	var windowWidth=windowSize().width;
	var windowHeight=windowSize().height;

	$(function() {
		$('.top-bar-dropdown-toggle').bind('click',function() {
			$('.top-left-logo').click();
		});

		$('body').on('dblclick', '.top-left-logo', function(e) {
			window.location.href='/';
		});
		$('.top-left-logo').bind('click',function(e) {
			e.stopPropagation();

			$('.search-filter-results').hide();
			$('.input-salary-filter-container').hide();
			$('.salary-filter-container').removeClass('active');
			$('.input-benefits-filter-container').hide();

			var top=$('.top-left-logo img').offset().top+$('.top-left-logo img').outerHeight()+7;
			var left=0;
			if(left<14) left=14;
			if(left>(windowSize().width-200)) left=(windowSize().width-200);
			if(top<14) top=14;
			if(top>(windowSize().height-200)) var top=(windowSize().height-200);
			$('.top-bar-dropdown').css('left',left+'px');
			$('.top-bar-dropdown').css('top',top+'px');
			$('html').toggleClass('top-bar-dropdown-expanded');
		});
		$('.filter-actions .top-bar-avatar').bind('dblclick',function(e) {
			e.stopPropagation();
			window.location.href='/';
		});

		$('.filter-actions .dropdown-toggle').bind('click',function() {
			$('.filter-actions .top-bar-avatar').click();
		});
		$('.filter-actions .top-bar-avatar').bind('click',function(e) {
			e.stopPropagation();
			$('.search-filter-results').hide();
			$('.input-salary-filter-container').hide();
			$('.salary-filter-container').removeClass('active');
			$('.input-benefits-filter-container').hide();

			$('.top-bar-dropdown').css('position','absolute');
			$('.top-bar-dropdown').css('left',$(this).offset().left);
			$('.top-bar-dropdown').css('top',$(this).offset().top+$(this).height()+7);
			$('html').toggleClass('top-bar-dropdown-expanded');
		});
	});
</script>
