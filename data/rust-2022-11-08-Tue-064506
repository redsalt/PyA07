<script>

	function windowSize() {
		var e = window, a = 'inner';
		if (!('innerWidth' in window )) {
				a = 'client';
				e = document.documentElement || document.body;
		}
		return { width : e[ a+'Width' ] , height : e[ a+'Height' ] };
	}
	var windowWidth=windowSize().width;
	var windowHeight=windowSize().height;

	$(function() {
		$('.top-bar-dropdown-toggle').bind('click',function() {
			$('.top-left-logo').click();
		});

		$('body').on('dblclick', '.top-left-logo', function(e) {
			window.location.href='/';
		});
		$('.top-left-logo').bind('click',function(e) {
			e.stopPropagation();

			$('.search-filter-results').hide();
			$('.input-salary-filter-container').hide();
			$('.salary-filter-container').removeClass('active');
			$('.input-benefits-filter-container').hide();

			var top=$('.top-left-logo img').offset().top+$('.top-left-logo img').outerHeight()+7;
			var left=0;
			if(left<14) left=14;
			if(left>(windowSize().width-200)) left=(windowSize().width-200);
			if(top<14) top=14;
			if(top>(windowSize().height-200)) var top=(windowSize().height-200);
			$('.top-bar-dropdown').css('left',left+'px');
			$('.top-bar-dropdown').css('top',top+'px');
			$('html').toggleClass('top-bar-dropdown-expanded');
		});
		$('.filter-actions .top-bar-avatar').bind('dblclick',function(e) {
			e.stopPropagation();
			window.location.href='/';
		});

		$('.filter-actions .dropdown-toggle').bind('click',function() {
			$('.filter-actions .top-bar-avatar').click();
		});
		$('.filter-actions .top-bar-avatar').bind('click',function(e) {
			e.stopPropagation();
			$('.search-filter-results').hide();
			$('.input-salary-filter-container').hide();
			$('.salary-filter-container').removeClass('active');
			$('.input-benefits-filter-container').hide();

			$('.top-bar-dropdown').css('position','absolute');
			$('.top-bar-dropdown').css('left',$(this).offset().left);
			$('.top-bar-dropdown').css('top',$(this).offset().top+$(this).height()+7);
			$('html').toggleClass('top-bar-dropdown-expanded');
		});
	});
</script>
